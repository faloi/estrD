#include <iostream>
#include "stack.h"

using namespace std;

void toString(Stack s) {
    cout << "TIENE ELEMENTOS?: " << boolalpha << !isEmpty(s) << endl;
    cout << "PRIMER ELEMENTO: " << top(s) << endl;
    cout << "SIZE: " << size(s) << " esta vacia " << endl;
}

int main()
{
    Stack s = emptyStack();

    push(s, 10);
    push(s, 20);
    push(s, 80);

    pop(s);

    toString(s);

    return 0;
}
