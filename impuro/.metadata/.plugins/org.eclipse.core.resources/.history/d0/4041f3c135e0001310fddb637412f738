#include <iostream>

struct QueueNode {
	int elem;
	QueueNode* next;
};

//INVARIANTE DE REPRESENTACION
//size representa la cantidad de elementos
//el next de last siempre es NULL
//el queue vacio se representa con (0, NULL, NULL)
struct QueueSt {
	int size;
	QueueNode* head;
	QueueNode* last;
};

QueueNode* singleton(int n, QueueNode* q);

Queue emptyQueue() {
	QueueSt* q = new QueueSt;
	q->head = NULL;
	q->last = NULL;
	q->size = 0;

	return q;
}

void enqueue(Queue& q, int n);
void dequeue(Queue& q); //pre !isEmpty(q)
void clear(Queue& q);

bool isEmpty(Queue q);
int first(Queue q); //pre !isEmpty(q)
int size(Queue q);
