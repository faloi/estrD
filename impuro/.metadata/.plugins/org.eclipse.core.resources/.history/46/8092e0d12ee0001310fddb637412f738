#include <iostream>
#include "stack.h"

using namespace std;

int main()
{
    Stack s = emptyStack();

    cout << "Agrego el 10" << endl;

    push(s, 10);

    cout << "Agrego el 20" << endl;

    push(s, 20);

    cout << "Saco un elemento" << endl;

    pop(s);

    // si tienen todo bien implementado
    // deberia mostrar el 10
    cout << "El primer elemento es " << top(s) << " y el size es " << size(s) << endl;

    return 0;
}
